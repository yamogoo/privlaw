                                                                            span.date-box__number=event.date.number
                                                                            div.date-box__group
                                                                                span.date-box__month=event.date.month
                                                                                span.date-box__day=event.date.day
                                        // Подать заявку на участие
                                        h3#section-3 Подать заявку на участие
                                        p Хотите принять участие как гость или выступить в качестве спикера?
                                        p Заполните форму ниже, и наш сотрудник с Вами обязательно свяжется.
                                        div.section
                                            div.section-body
                                                div.form-wrapper 
                                                    form.form#form-educational-projects(action="#")
                                                        div.form-container
                                                            div.form-body
                                                                div.form-group.columns
                                                                    div.form__field
                                                                        label(for="name") ФИО *
                                                                        input(type="text" name="name" id="name" required)
                                                                    div.form__field
                                                                        label(for="email") Email *
                                                                        input(type="email" name="email" id="email" required)
                                                                div.form-group.columns
                                                                    div.form__field
                                                                        label(for="phone") Телефон *
                                                                        input(type="phone" name="phone" id="phone" required)
                                                                    div.form__field
                                                                        label(for="place") Место проведения конференции *
                                                                        input(type="text" name="place" id="place" required)
                                                                div.form-group.columns
                                                                    div.form__field
                                                                        label(for="work") Место работы *
                                                                        input(type="text" name="work" id="work" required)
                                                                    div.form__field
                                                                        label(for="position") Должность *
                                                                        input(type="text" name="position" id="position" required)
                                                                div.form__field
                                                                    label(for="city") Город *
                                                                    input(type="text" name="city" id="city" required)
                                                                div.form__field
