// Personal Area
section.section
    div.section-container
        //Nav Line
        div.nav-line
            ul.nav-line-container
                li.nav-line__item
                    a(href="index.html") Главная
                li.nav-line__item
                    a(href="personal-area_schedule-and-curriculam.html") Личный кабинет
                li.nav-line__item
                    a(href="personal-area_lectures.html") Лекции
                li.nav-line__item
                    a(href="personal-area_lectures.html") 2022
        //Nav Line
        // Section Nav
        div.section-nav-wrapper
            div.section-nav--primary
                ul.section-nav-container
                    li.section-nav__item
                        a(href="personal-area_schedule-and-curriculam.html") Расписание и учебные планы
                    li.section-nav__item.active
                        a(href="personal-area_lectures.html") Лекции
                    li.section-nav__item
                        a(href="personal-area_documents.html") Материалы и документы
        // Section Header
        div.section-header
            h1.section-title Лекции
        // Section Nav
        div.section-nav-wrapper
            div.section-nav--tritic
                ul.section-nav-container
                    each val in [1999, 2000, 2001, 2002, 2003, 2004, 2005, 2006, 2007, 2008, 2009, 2010, 2011, 2012, 2013, 2014, 2015, 2016, 2017, 2018, 2019, 2020, 2021, 2022]
                        if val == 2022
                            li.section-nav__item.active
                                a(href=`rshchp_graduates_${val}.html`)=val
                        else
                            li.section-nav__item
                                a(href=`rshchp_graduates_${val}.html`)=val
        div.section-body
            div.link-list
                ul.link-list-container
                    //Items
                    each item in teachers
                        li.link-list__item
                            span.link-list__item-icon
                            a.link-list__item-link(href=`${item.url}`)=item.name